<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.infogen.ims.mapper.UserMapper">
    <select id="userInfoSelect" parameterType="String" resultType="com.infogen.ims.common.CamelMap">
        SELECT *
          FROM TB_AUTH_USER
         WHERE 1=1
           AND USER_NM like '%${userNm}%'
    </select>    
</mapper>